page{
  background: #f5f5f5;
  border-top: 1rpx solid #e5e5e5;
}
.queList{
  width: 100%;
  height: auto;
  margin: 120rpx 0 20rpx;
}
.queList .queItem{
  width: 710rpx;
  height: auto;
  margin: 20rpx 20rpx 0;
  background-color: #ffffff;
  padding:10rpx 24rpx;
  box-sizing: border-box;
  border-radius:8rpx ;
}
.queItem>view:nth-child(1){
  font-size: 28rpx;
  line-height: 40rpx;
}
/* 去登录页面 */
.bgImg {
  width: 508rpx;
  height: 264rpx;
  opacity: 0.7;
  margin: 265rpx 121rpx 24rpx;
}

.loginTitle {
  width: 100%;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  line-height: 40rpx;
  text-align: center;
  display: block;
}

.gologinBtn {
  width: 490rpx;
  height: 80rpx;
  background: linear-gradient(90deg, #0e4090, #1890ff);
  border-radius: 50rpx;
  font-size: 38rpx;
  margin: 100rpx 130rpx 0;
  color: #ffffff;
  text-align: center;
  line-height: 80rpx;
}